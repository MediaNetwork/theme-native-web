<!doctype html>
<html lang="en">
<head>
	<title><% content 'title' %></title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="TopGirls VIP has the best beautiful pics on the Internet!!!">

	<!-- Progressive Web -->
	<link rel="manifest" href="<%= @asset('/js/manifest.json') %>">
	<meta name="msapplication-TileColor" content="#000000">
	<meta name="theme-color" content="#000000">

	<!-- iOS -->
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="black">
	<meta name="format-detection" content="telephone=no">

	<!-- Favicon -->
	<% include 'partials/favicon' %>
	<!-- FB meta -->
	<% content 'fb-meta' %>

	<link rel="prev" title="Previous" href="<%= @prev %>">
	<link rel="next" title="Next" href="<%= @next %>">

	<% if @config.production: %>
		<link rel="stylesheet" href="<%= @asset('/css/theme.min.css') %>">
	<% else: %>
		<link rel="stylesheet" href="<%= @asset('/css/reset.css') %>">
		<link rel="stylesheet" href="<%= @asset('/css/spn.css') %>">
		<link rel="stylesheet" href="<%= @asset('/css/style.css') %>">
		<link rel="stylesheet" href="<%= @asset('/css/responsive.css') %>">
	<% end %>

	<% if @setting['ci_header']: %>
		<%- @setting['ci_header'] %>
	<% end %>
</head>
<body>
	<header id="header" class="cf">
		<div id="logo" class="large monitor only">
			<a href="/" class="brand-logo" title="<%= @settings['page_title'] || 'Media Feed' %>">
				<img src="<%= @asset('/img/logo.png') %>" alt="<%= @settings['page_title'] || 'Image Feed' %>">
			</a>
		</div>
		<div class="header ad">
			<% if @config.production: %>
				<% include 'partials/adsense-ad-header' %>
			<% end %>
		</div>
	</header>
	<div id="wrapper">
		<% content %>
	</div>
	<section id="footer">
		<span class="copyright">&copy; 2017</span>
		<span class="powered by">Powered by <a href="https://github.com/MediaSocialNetwork">MediaSocialNetwork</a></span>
	</section>

	<div id="fb-root"></div>
	<% content 'scripts' %>

	<% if @setting['ci_footer']: %>
		<%- @setting['ci_footer'] %>
	<% end %>
</body>
</html>
